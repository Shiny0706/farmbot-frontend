{
  "name": "farmbot-web-frontend",
  "version": "0.2.0",
  "description": "Farmbot web frontend.",
  "main": "dist/fb_frontend.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/farmbot/farmbot-web-frontend"
  },
  "scripts": {
    "build": "npm run clean && npm run build:dev && npm run build:prd",
    "build:dev": "webpack src/fb_frontend.tsx dist/fb_frontend.js --config tools/webpack.config.dev.js",
    "build:prd": "webpack src/fb_frontend.tsx dist/fb_frontend.min.js --config tools/webpack.config.prd.js",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --config tools/webpack.config.dev.js",
    "clean": "rimraf dist build",
    "lint": "eslint src",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "farmbot"
  ],
  "author": "farmbot.io",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "loader-utils": "^0.2.12",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "farmbot": "0.3.2",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "toastr": "^2.1.2"
  }
}
